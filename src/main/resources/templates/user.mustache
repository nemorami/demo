<form name="user" method="post" action="/create-user">
    <div class="form-group">
        <label for="userId">사용자 아이디</label>
        <input class="form-control" name="userId" value="{{user.userId}}" placeholder="User ID">
    </div>
    <div class="form-group">
        <label for="password">비밀번호</label>
        <input type="password" class="form-control" name="password" placeholder="Password">
        <input type="password" name="password2" >
    </div>
    <div class="form-group">
        <label for="name">이름</label>
        <input class="form-control" name="name" placeholder="Name">
    </div>
    <div class="form-group">
        <label for="email">guid</label>
        <input  class="form-control" name="guid" placeholder="Email">
    </div>
    <button type="submit" class="btn btn-success clearfix pull-right">Submit</button>
    <div class="clearfix" />
</form>
